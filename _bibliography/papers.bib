@article{street_photoconductivity_2011,
 title = {Photoconductivity measurements of the electronic structure of organic solar cells},
 volume = {83},
 copyright = {All rights reserved},
 issn = {1098-0121, 1550-235X},
 url = {https://link.aps.org/doi/10.1103/PhysRevB.83.165207},
 doi = {10.1103/PhysRevB.83.165207},
 language = {en},
 number = {16},
 urldate = {2019-09-12},
 journal = {Physical Review B},
 author = {Street, R. A. and Song, K. W. and Northrup, J. E. and Cowan, S.},
 month = apr,
 year = {2011},
 pages = {165207},
 category = {solarcells},
 pdf = {https://drive.google.com/file/d/1BvkymB6X3-vuPMB5lJYITWEZ8hQy2V2e/view?usp=sharing},
 abstract = {Experimental and theoretical studies of the electronic structure of bulk heterojunction (BHJ) organic solar cells are reported. The photoconductivity spectral response of the solar cells has a weak absorption band extending from the band-gap energy down to <1 eV due to charge-transfer optical excitation at the interface between the polymer and the fullerene. The low-energy absorption indicates an exponential band tail of localized states and an absorption model based on the one-electron joint density of electronic states accounts for the data. Transient photoconductivity measurements of the carrier mobility exhibit a temperature-dependent carrier dispersion. Data analysis for the particular case of transport in the BHJ structure is developed. A multiple trapping model of the dispersive transport is consistent with localized band tail states having a comparable density-of-states distribution to those observed by optical absorption. Theoretical calculations of the density of states including disorder in the π-π spacing of the polymer chains also shows exponential band tailing. A density-of-states model is developed from the data and is discussed.}
}

@inproceedings{hu_high-resolution_2012,

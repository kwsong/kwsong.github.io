@article{han_17.3:_2010,
 title = {17.3: a-{Si}:{H} {Thin}-film {Transistors} with a {New} {Hybrid} {Dielectric} {Highly} {Stable} under {Mechanical} and {Electrical} {Stress}},
 volume = {41},
 copyright = {All rights reserved},
 issn = {0097-966X},
 shorttitle = {17.3},
 url = {http://doi.wiley.com/10.1889/1.3500416},
 doi = {10.1889/1.3500416},
 abstract = {A resilient, homogeneous, hybrid of silicon dioxide and silicone polymer deposited at room-temperature replaces the conventional, brittle, silicon nitride barrier layer and gate insulator in amorphous silicon thin-film transistors(a-Si:H TFTs) on flexible polyimide foil. The electron field-effect mobility is high at 1.6 cm2/V⋅s, and the threshold voltage shift under high gate bias is comparable to that in conventional a-Si:H/SiNx TFTs fabricated at 300oC. The substhreshold slope is 290 mV/decade. The new transistors are highly flexible, as they can be bent down to 0.5 mm radius (5\% strain) in tension and down to 1mm radius (2.5\% strain) in compression. These TFTs qualify for roll-out screens of hand-held devices.},
 language = {en},
 number = {1},
 urldate = {2019-09-12},
 journal = {SID Symposium Digest of Technical Papers},
 author = {Han, Lin and Song, Katherine and Wagner, Sigurd and Mandlik, Prashant},
 year = {2010},
 pages = {238},
 pdf = {https://drive.google.com/file/d/1TfYsm1FuoBOdeehqdwe-tOaxTryJKaGF/view?usp=sharing},
 category = {TFTs}
}


@inproceedings{han_invited_2010,
 address = {Las Vegas, NV},
 title = {({Invited}) {A} {New} {Insulator} for {Thin}-{Film} {Transistor} {Backplanes} and for {Flexible} {Passivation} {Layers}},
 copyright = {All rights reserved},
 url = {http://ecst.ecsdl.org/cgi/doi/10.1149/1.3481226},
 doi = {10.1149/1.3481226},
 language = {en},
 urldate = {2019-09-12},
 author = {Han, Lin and Song, Katherine and Wagner, Sigurd and Mandlik, Prashant},
 booktitle = {ECS Transactions},
 year = {2010},
 volume = {33},
 number = {5},
 pages = {125--134},
 pdf = {https://drive.google.com/file/d/18FNIfMMdohIimUWO9aTR5H6TA2v0VWwE/view?usp=sharing},
 category = {TFTs},
 abstract = {We describe a new flexible material that functions as a gate dielectric of amorphous-silicon thin-film transistors, as a passivation layer for plastic substrates, and as an environmental barrier for organic light-emitting diodes. The material is SiO2 with some silicone polymer character. It is deposited, typically at room temperature, from a glow discharge in which a silicone precursor molecule, hexamethyl disiloxane, is oxidized. Films of the material can be made with a broad range of properties, which vary from those of SiO2 to those of plasma-polymerized silicone. Many macroscopic properties of these films suggest that they are homogeneous. We call the films "hybrid."}
}

@article{han_ultraflexible_2010,

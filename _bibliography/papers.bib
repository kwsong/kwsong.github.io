 title = {({Invited}) {A} {New} {Insulator} for {Thin}-{Film} {Transistor} {Backplanes} and for {Flexible} {Passivation} {Layers}},
 copyright = {All rights reserved},
 url = {http://ecst.ecsdl.org/cgi/doi/10.1149/1.3481226},
 doi = {10.1149/1.3481226},
 language = {en},
 urldate = {2019-09-12},
 author = {Han, Lin and Song, Katherine and Wagner, Sigurd and Mandlik, Prashant},
 booktitle = {ECS Transactions},
 year = {2010},
 volume = {33},
 number = {5},
 pages = {125--134},
 pdf = {https://drive.google.com/file/d/18FNIfMMdohIimUWO9aTR5H6TA2v0VWwE/view?usp=sharing},
 category = {TFTs},
 abstract = {We describe a new flexible material that functions as a gate dielectric of amorphous-silicon thin-film transistors, as a passivation layer for plastic substrates, and as an environmental barrier for organic light-emitting diodes. The material is SiO2 with some silicone polymer character. It is deposited, typically at room temperature, from a glow discharge in which a silicone precursor molecule, hexamethyl disiloxane, is oxidized. Films of the material can be made with a broad range of properties, which vary from those of SiO2 to those of plasma-polymerized silicone. Many macroscopic properties of these films suggest that they are homogeneous. We call the films "hybrid."}
}


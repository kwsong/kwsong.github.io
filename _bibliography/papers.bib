@article{street_photoconductivity_2011,
 abbr = {Phys Rev B},
 title = {Photoconductivity measurements of the electronic structure of organic solar cells},
 volume = {83},
 copyright = {All rights reserved},
 issn = {1098-0121, 1550-235X},
 url = {https://link.aps.org/doi/10.1103/PhysRevB.83.165207},
 doi = {10.1103/PhysRevB.83.165207},
 language = {en},
 number = {16},
 urldate = {2019-09-12},
 journal = {Physical Review B},
 author = {Street, R. A. and Song, K. W. and Northrup, J. E. and Cowan, S.},
 month = apr,
 year = {2011},
 pages = {165207},
 category = {solarcells},
 pdf = {https://drive.google.com/file/d/1BvkymB6X3-vuPMB5lJYITWEZ8hQy2V2e/view?usp=sharing},
 abstract = {Experimental and theoretical studies of the electronic structure of bulk heterojunction (BHJ) organic solar cells are reported. The photoconductivity spectral response of the solar cells has a weak absorption band extending from the band-gap energy down to <1 eV due to charge-transfer optical excitation at the interface between the polymer and the fullerene. The low-energy absorption indicates an exponential band tail of localized states and an absorption model based on the one-electron joint density of electronic states accounts for the data. Transient photoconductivity measurements of the carrier mobility exhibit a temperature-dependent carrier dispersion. Data analysis for the particular case of transport in the BHJ structure is developed. A multiple trapping model of the dispersive transport is consistent with localized band tail states having a comparable density-of-states distribution to those observed by optical absorption. Theoretical calculations of the density of states including disorder in the π-π spacing of the polymer chains also shows exponential band tailing. A density-of-states model is developed from the data and is discussed.}
}

@inproceedings{hu_high-resolution_2012,
 abbr = {VLSIC},
 address = {Honolulu, HI, USA},
 title = {High-resolution sensing sheet for structural-health monitoring via scalable interfacing of flexible electronics with high-performance {ICs}},
 copyright = {All rights reserved},
 isbn = {978-1-4673-0849-6 978-1-4673-0848-9 978-1-4673-0845-8},
 url = {http://ieeexplore.ieee.org/document/6243819/},
 doi = {10.1109/VLSIC.2012.6243819},
 abstract = {Early-stage damage detection for buildings and bridges re­ quires continuously sensing and assessing strain over large sur­ faces, yet with centimeter-scale resolution. To achieve this, we present a sensing sheet that combines high-performance ICs with flexible electronics, allowing bonding to such surfaces. The flexi­ ble electronics integrates thin-film strain gauges and amorphous­ silicon control circuits, patterned on a polyimide sheet that can potentially span large areas. Non-contact links couple digital and analog signals to the ICs, allowing many ICs to be intro­ duced via low-cost sheet lamination for energy-efficient readout and computation over a large number of sensors. Communi­ cation between distributed ICs is achieved by transceivers that exploit low-loss interconnects patterned on the polyimide sheet; the transceivers self-calibrate to the interconnect impedance to maximize transmit SNR. The system achieves multi-channel strain readout with sensitivity of 18J1'strainRMs at an energy per measurement of 270nJ, while the communication energy is 12.8pJ/3.3pJ per bit (Tx/Rx) over 7.5m.},
 language = {en},
 urldate = {2019-09-12},
 booktitle = {2012 {Symposium} on {VLSI} {Circuits} ({VLSIC})},
 publisher = {IEEE},
 author = {Hu, Yingzhe and Rieutort-Louis, Warren and Sanz-Robinson, Josue and Song, Katherine and Sturm, James C. and Wagner, Sigurd and Verma, Naveen},
 month = jun,
 year = {2012},
 pages = {120--121},
 category = {TFTs},
 pdf = {https://drive.google.com/file/d/1ko-6UpGVggKiVRiVI_XDaC3w87tHEddE/view?usp=sharing}
}

@article{supran_qleds_2013,
